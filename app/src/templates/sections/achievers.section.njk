<div class="container">
    <div class="text-center" uk-parallax="opacity:0,1; y:120,0;viewport:0.3">
        <h2 class="section-title">Achievers</h2>
        <div class="section-title-underline"></div>
    </div>
    <div class="row justify-content-center">
        {% for achiever in content.achievers.achievers %}
            <div class="col-6 col-md-4 p-3 p-md-5">
                <img src="assets/images/faces/avatar.png" alt="John Doe" class="achiever-img img-fluid rounded-circle" data-aos="zoom-in">
                <div class="achiever-details text-center pt-2 pt-md-4">
                    <div class="font-weight-bold achiever-name" data-aos="fade-up">John Doe</div>
                    <div class="text-muted achiever-place" data-aos="fade-up" data-aos-delay="300">New York</div>
                </div>
            </div>
        {% endfor %}
    </div>
    <div class="text-center text-md-right p-5">
        <a href="#" class="btn btn-primary font-weight-bold" data-aos="fade-right">
            View More <i class="fa fa-angle-double-right"></i>
        </a>
    </div>
</div>